sudo cp -r local/*  /var/lib/lxc/target/rootfs/usr/local/
sudo cp -r opt/*  /var/lib/lxc/target/rootfs/opt/

sudo cp -r local/*  /var/lib/lxc/template/rootfs/usr/local/
sudo cp -r opt/*  /var/lib/lxc/template/rootfs/opt/


